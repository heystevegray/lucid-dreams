{
    "name": "lucid-dreams",
    "version": "1.0.11-2",
    "description": "A CLI to convert a Lucidchart ERD to a Prisma schema file",
    "main": "src/cli.ts",
    "bin": {
        "lucid-dreams": "bin/lucid-dreams"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib",
        "bin"
    ],
    "scripts": {
        "dev": "nodemon --exec npm run build",
        "create": "npm run build && npm run test",
        "build": "rm -rf lib && tsc -p .",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
        "format": "prettier --write .",
        "release": "npm run build && npx np"
    },
    "keywords": [
        "cli",
        "lucid-dreams"
    ],
    "author": "Steve",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/heystevegray/lucid-dreams"
    },
    "homepage": "https://github.com/heystevegray/lucid-dreams",
    "dependencies": {
        "arg": "^5.0.0",
        "csv-parser": "^3.0.0",
        "esm": "^3.2.25",
        "listr": "^0.14.3"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/listr": "^0.14.2",
        "@types/node": "^14.14.41",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "prettier": "2.2.1",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
